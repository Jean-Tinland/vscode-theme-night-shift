  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#98a8c5"
      }
    },
    {
      "name": "Constants",
      "scope": "constant",
      "settings": {
        "foreground": "#ad82cb"
      }
    },
    {
      "name": "Escape characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#6db3ce"
      }
    },
    {
      "name": "Named entities",
      "scope": "entity.name",
      "settings": {
        "foreground": "#fff9ee"
      }
    },
    {
      "name": "Function names",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffd484"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#7eddde"
      }
    },
    {
      "name": "Static types",
      "scope": ["entity.name.type", "storage.type.cs"],
      "settings": {
        "foreground": "#ffd484"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#e78482"
      }
    },
    {
      "name": "CSS class selector",
      "scope": "entity.other.attribute-name.class.css",
      "settings": {
        "fontStyle": "",
        "foreground": "#7eddde"
      }
    },
    {
      "name": "CSS transition name",
      "scope": "support.type.property-name.css",
      "settings": {
        "fontStyle": "",
        "foreground": "#ffd484"
      }
    },
    {
      "name": "CSS ID selectors",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "fontStyle": "",
        "foreground": "#ffd484"
      }
    },
    {
      "name": "CSS colors",
      "scope": "constant.other.color.rgb-value.hex.css",
      "settings": {
        "foreground": "#ffd484"
      }
    },
    {
      "name": "SCSS include & mixins params",
      "scope": ["meta.at-rule.mixin", "meta.at-rule.include"],
      "settings": {
        "foreground": "#ffd484"
      }
    },
    {
      "name": "SCSS include & mixins parenthesis",
      "scope": [
        "punctuation.definition.parameters.begin.bracket.round",
        "punctuation.definition.parameters.end.bracket.round"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#fff9ee"
      }
    },
    {
      "name": "Inherited classes",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#ffd484"
      }
    },
    {
      "name": "CSS constant",
      "scope": "constant.numeric.css",
      "settings": {
        "foreground": "#8fc8bb"
      }
    },
    {
      "name": "CSS units",
      "scope": "keyword.other.unit",
      "settings": {
        "fontStyle": "",
        "foreground": "#fff9ee"
      }
    },
    {
      "name": "Pseudo CSS",
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#e78482"
      }
    },
    {
      "name": "Variable names",
      "scope": ["entity.name.variable", "variable"],
      "settings": {
        "foreground": "#fff9ee"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#ad82cb"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#6db3ce"
      }
    },
    {
      "name": "Special operators",
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.operator.delete"
      ],
      "settings": {
        "foreground": "#6db3ce"
      }
    },
    {
      "name": "Markup quotes",
      "scope": "markup.quote",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6db3ce"
      }
    },
    {
      "name": "Markup headings",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#e78482"
      }
    },
    {
      "name": "Markup bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ad82cb"
      }
    },
    {
      "name": "Markup italics",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7eddde"
      }
    },
    {
      "name": "Markup code",
      "scope": ["markup.inline.raw", "markup.fenced_code.block"],
      "settings": {
        "foreground": "#e78482"
      }
    },
    {
      "name": "Markup links",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#ffd484"
      }
    },
    {
      "name": "Storage keywords",
      "scope": "storage",
      "settings": {
        "foreground": "#fff9ee"
      }
    },
    {
      "name": "Strings",
      "scope": ["string.quoted", "string.template"],
      "settings": {
        "foreground": "#98a8c5"
      }
    },
    {
      "name": "Regex",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#fff9ee"
      }
    },
    {
      "name": "Markup link descriptions",
      "scope": "string.other.link",
      "settings": {
        "foreground": "#fff9ee"
      }
    },
    {
      "name": "Provided (support) values",
      "scope": "support",
      "settings": {
        "fontStyle": "",
        "foreground": "#6db3ce"
      }
    },
    {
      "name": "Provided functions",
      "scope": "support.function",
      "settings": {
        "foreground": "#8fc8bb"
      }
    },
    {
      "name": "Provieded variables",
      "scope": "support.variable",
      "settings": {
        "foreground": "#8fc8bb"
      }
    },
    {
      "name": "Object/JSON property names",
      "scope": ["support.type.property-name", "meta.object-literal.key"],
      "settings": {
        "foreground": "#ffd484"
      }
    },
    {
      "name": "CSS property names",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#fff9ee"
      }
    },
    {
      "name": "Language variables",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#ffd484"
      }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#fff9ee"
      }
    },
    {
      "name": "Embedded template punctuation",
      "scope": "string.template meta.embedded",
      "settings": {
        "foreground": "#fff9ee"
      }
    },
    {
      "name": "Tag brackets",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#98a8c5"
      }
    },
    {
      "name": "Key/value separators",
      "scope": "punctuation.separator",
      "settings": {
        "foreground": "#98a8c5"
      }
    },
    {
      "name": "Template expressions",
      "scope": "punctuation.definition.template-expression",
      "settings": {
        "foreground": "#f5f5f5"
      }
    },
    {
      "name": "Embedded section punctuation",
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#ad82cb"
      }
    },
    {
      "name": "Markup list punctuation",
      "scope": "punctuation.definition.list",
      "settings": {
        "foreground": "#e78482"
      }
    }
  ]
}
